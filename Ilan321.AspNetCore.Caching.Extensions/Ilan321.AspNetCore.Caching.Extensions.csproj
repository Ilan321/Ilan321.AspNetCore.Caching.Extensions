<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Title>Ilan321.AspNetCore.Caching.Extensions</Title>
    <Authors>Ilan321</Authors>
    <Description>A set of useful extensions for Microsoft cache services.</Description>
    <Copyright>Copyright (c) Ilan321 2022</Copyright>
    <PackageProjectUrl>https://github.com/Ilan321/Ilan321.AspNetCore.Caching.Extensions</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Ilan321/Ilan321.AspNetCore.Caching.Extensions</RepositoryUrl>
    <PackageTags>caching,microsoft,extensions,ilan321</PackageTags>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Caching.Abstractions" Version="6.0.0" />
    <PackageReference Include="System.Text.Json" Version="6.0.5" />
  </ItemGroup>

</Project>
